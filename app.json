{
  "expo": {
    "name": "FreeFit",
    "slug": "freefit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/freefit_icon.png",
    "scheme": "workoutlogapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/freefit_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/freefit_icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lantenweid.freefit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/freefit_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FF990E"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c53735b8-ce0e-40bb-9dd2-cc7bd505bdd7"
      }
    }
  }
}
